{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Front-end for https://github.com/wrightstream/landing",
  "main": "./public/index.html",
  "keywords": [
    "html",
    "css",
    "sass",
    "javascript",
    "react",
    "redux",
    "bulma",
    "axios"
  ],
  "author": [
    "Claudia Ligidakis <claudialigidakis@gmail.com>",
    "Vikanda Gonzales <vikanda.gonzales@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "bulma-extensions": "^4.0.0",
    "moment": "^2.22.2",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14"
  },
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/stylesheets/sass/main.scss src/stylesheets/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/stylesheets/css/main.css src/stylesheets/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch | npm run js-watch | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
